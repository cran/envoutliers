citHeader("To cite envoutliers in publications use:")

citEntry(entry = "Unpublished",
  title        = "An R Package for Identification of Outliers in Environmental Data",
  author       = personList(
                 as.person("Campulova Martina"),
                 as.person("Campula Roman")),
  year         = "2019",
  note         = "To be published",
  textVersion  =
  paste("Campulova Martina, Campula Roman (2019).",
        "An R Package for Identification of Outliers in Environmental Data.")
)
